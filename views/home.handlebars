<div class="p-5">
    <table class="table">
        <thead>
            <tr>
                <th>Nombre</th>
                <th>Email</th>
                <th>Fecha de Creación</th>
                <th>Acción</th>
            </tr>
        </thead>
        <tbody>
            {{#each usuarios}}
            <tr>
                <td>{{this.username}}</td>
                <td>{{this.email}}</td>
                <td>{{this.date}}</td>
                <td><a href="/user-delete/{{this.id}}" class="btn btn-danger">Eliminar</a></td>
            </tr>
            {{/each}}

        </tbody>
    </table>


    <a href="/user-create" class="btn btn-primary">Crear</a>
</div>